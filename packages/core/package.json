{
  "name": "@uniresp/core",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./response": {
      "types": "./dist/response.d.ts",
      "import": "./dist/response.js",
      "require": "./dist/response.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "keywords": [
    "api",
    "response",
    "error",
    "handling",
    "node",
    "express",
    "nextjs",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tien2003deptrai/uniresp_refs.git"
  },
  "bugs": {
    "url": "https://github.com/Tien2003deptrai/uniresp_refs/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "tsc -b",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "typescript": "^5.5.0"
  },
  "description": "Core types and helpers for unified API responses."
}

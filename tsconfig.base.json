{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitOnError": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "composite": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@uniresp/core": [
        "packages/core/src"
      ],
      "@uniresp/errors": [
        "packages/errors/src"
      ]
    }
  }
}